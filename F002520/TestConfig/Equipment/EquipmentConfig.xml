<?xml version="1.0" encoding="utf-8" ?>
<Configuration>

  <DeviceList>

    <DAQ>
      <Enable>TRUE</Enable>
      <!-- NI | JY-DAM-->
      <Type>NI</Type>
      <Name>Dev1</Name>
    </DAQ>

    <MOTOR>
      <Enable>TRUE</Enable>
      <!-- OMORN | PANASONIC-->
